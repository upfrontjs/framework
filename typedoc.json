{
    "entryPoints": [
        "src/index.ts"
    ],
    "tsconfig": "tsconfig.json",
    "out": "api-docs",
    "name": "UpfrontJS",
    "readme": "./README.md",
    "plugin": [],
    "includeVersion": true,
    "cleanOutputDir": true,
    "intentionallyNotExported": [
        "WhereDescription",
        "Order",
        "Method",
        "Operator",
        "Direction",
        "Relation",
        "WithProperty",
        "BooleanOperator",
        "BuiltInCastType"
    ],
    "validation": {
        "notExported": true,
        "invalidLink": false
    }
}
